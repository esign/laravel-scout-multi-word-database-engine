{
    "name": "esign/laravel-scout-multi-word-database-engine",
    "description": "Laravel Scout database driver to match multiple words separately",
    "keywords": [
        "esign",
        "scout-multi-word-database-engine"
    ],
    "homepage": "https://github.com/esign/laravel-scout-multi-word-database-engine",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jordy Van der Haegen",
            "email": "jordy@esign.eu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^11.0",
        "laravel/scout": "^10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Esign\\ScoutMultiWordDatabaseEngine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Esign\\ScoutMultiWordDatabaseEngine\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpcs": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Esign\\ScoutMultiWordDatabaseEngine\\ScoutMultiWordDatabaseEngineServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
